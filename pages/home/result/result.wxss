.container {
  height: 100%;
  width: 100%
}

.innercontainer {
  display: flex;
  flex-direction: column;
  height: 450rpx;
  width: 90%;
  margin: 60rpx auto;
  border-radius: 20px;
  border: solid #cdcdcd 0.5rpx;
  /*box-shadow:  20rpx 20rpx 0rpx 20rpx #cdcdcd inset;*/
}

.time {
  margin: 0rpx 50rpx;
  padding: 25rpx 0rpx;
  font-size: 30rpx;
  border-bottom: dashed #cdcdcd 0.5rpx;
}

.test {
  
  /*transform: scale(0.5)*/
}
.result {
  display: flex;
  flex-direction: row;
  
}


.score {
  display: flex;
  flex-direction: column;
  width: 50%;
}

.text {
  font-size: 30rpx;
  margin: 25rpx 50rpx 0rpx 50rpx ;
}
.detail {
  display: flex;
  flex-direction: column;
  padding: 100rpx 0rpx 50rpx 50rpx;
 
}
.dot {
  height: 20rpx;
  width: 20rpx;
  border-radius: 50%;
  background-color: #7cb5ec;
  margin: auto;
}
.textcontainer {
  display: flex;
  flex-direction: row;
  
  height: 50rpx;
  
}
.text1{
  font-size: 30rpx;
  margin: auto 20rpx;
 
}